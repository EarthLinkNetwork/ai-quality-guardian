{
  "name": "pm-orchestrator-runner",
  "version": "1.0.26",
  "description": "CLI tool that enforces controlled execution of Claude Code through mandatory orchestration",
  "main": "dist/cli/index.js",
  "bin": {
    "pm": "dist/cli/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "postbuild": "node scripts/generate-build-meta.js",
    "test": "mocha --require ts-node/register 'test/**/*.test.ts'",
    "test:unit": "mocha --require ts-node/register 'test/unit/**/*.test.ts'",
    "test:property": "mocha --require ts-node/register 'test/property/**/*.test.ts'",
    "test:integration": "mocha --require ts-node/register 'test/integration/**/*.test.ts'",
    "test:acceptance": "mocha --require ts-node/register 'test/acceptance/**/*.test.ts'",
    "coverage": "nyc npm test",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "typecheck": "tsc --noEmit",
    "llm:sentinel": "ts-node src/cli/llm-sentinel.ts",
    "llm:test:real": "LLM_TEST_MODE=1 mocha --require ts-node/register 'test/integration/real-llm-mediation.test.ts' 'test/integration/verdict-gate-open.test.ts'",
    "llm:test:verdict": "mocha --require ts-node/register 'test/unit/mediation/verdict-reporter.test.ts'",
    "e2e:smoke": "ts-node scripts/e2e-smoke.ts",
    "e2e:recovery": "ts-node scripts/e2e-recovery.ts",
    "proof:real": "ts-node scripts/e2e-real-world-proof.ts",
    "dynamodb:local:init": "ts-node scripts/dynamodb-local-init.ts",
    "audit:test": "bash scripts/audit/run-tests.sh",
    "audit:dist-docs-only": "bash scripts/audit/dist-docs-only.sh",
    "gate:ui": "ts-node diagnostics/ui-invariants.check.ts",
    "gate:task": "ts-node diagnostics/task-state.check.ts",
    "gate:web": "ts-node diagnostics/settings-ui.check.ts",
    "gate:agent": "ts-node diagnostics/agent-launcher.check.ts",
    "gate:tier0": "npm run gate:ui && npm run gate:task",
    "gate:incomplete": "ts-node diagnostics/read-info-incomplete.check.ts",
    "gate:import": "ts-node diagnostics/directory-import.check.ts",
    "gate:e2e-exec": "ts-node diagnostics/e2e-execution.check.ts",
    "gate:flake-guard": "ts-node diagnostics/flake-guard.check.ts",
    "gate:auth": "ts-node diagnostics/auth.check.ts",
    "gate:logs-visible": "ts-node diagnostics/logs-visible.check.ts",
    "gate:timeout-real": "ts-node diagnostics/timeout-real.check.ts",
    "gate:blocked-scope": "ts-node diagnostics/blocked-scope.check.ts",
    "gate:p0": "npm run gate:logs-visible && npm run gate:timeout-real && npm run gate:blocked-scope",
    "gate:p0-stale-final": "bash scripts/p0-stale-final.sh",
    "gate:all": "npm run gate:import && npm run gate:auth && npm run gate:p0 && npm run gate:p0-stale-final && npm run gate:tier0 && npm run gate:web && npm run gate:agent && npm run gate:spec && npm run gate:incomplete && npm run gate:e2e-exec && npm run gate:browser && npm run gate:flake-guard",
    "gate:spec": "ts-node diagnostics/spec-coverage.check.ts",
    "gate:chat": "ts-node diagnostics/chat-tasktype.check.ts",
    "gate:browser": "ts-node diagnostics/browser-ui.check.ts",
    "test:playwright": "npx playwright test",
    "test:playwright:headed": "npx playwright test --headed"
  },
  "keywords": [
    "pm-orchestrator",
    "claude-code",
    "runner"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/chai": "^5.2.3",
    "@types/express": "^5.0.6",
    "@types/minimatch": "^5.1.2",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "chai": "^6.2.2",
    "eslint": "^8.55.0",
    "fast-check": "^3.15.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "playwright": "^1.58.1",
    "pm-orchestrator-runner": "^1.0.23",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.971.0",
    "@aws-sdk/lib-dynamodb": "^3.971.0",
    "@types/js-yaml": "^4.0.9",
    "commander": "^12.0.0",
    "express": "^5.2.1",
    "js-yaml": "^4.1.1",
    "minimatch": "^9.0.0",
    "uuid": "^10.0.0",
    "yaml": "^2.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
