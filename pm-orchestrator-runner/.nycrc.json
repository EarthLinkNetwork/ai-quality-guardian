{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "include": ["src/**/*.ts"],
  "exclude": ["src/**/*.d.ts", "test/**/*"],
  "reporter": ["text", "html", "lcov"],
  "check-coverage": true,
  "lines": 90,
  "branches": 85,
  "functions": 90,
  "statements": 90,
  "report-dir": "coverage"
}
